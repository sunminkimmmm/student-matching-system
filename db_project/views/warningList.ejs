<h4>신고 목록</h4>
<table class="table table-hover">
    <thead>
        <tr>
          <th scope="col">팀 번호</th>
          <th scope="col">신고된 아이디</th>
          <th scope="col">신고한 아이디</th>
          <th scope="col">신고날짜</th>
        </tr>
      </thead>
<tbody>
    <% for(var i = 0; i< data.length; i++){%>
        <% if(data[i].warning_state == 0) {%>
            <tr>
                    <td>
                        <a href="/warningListDetail/<%=data[i].warning_code%>">
                        <%= data[i].team_code %>
                        </a>
                    </td>
                    <td>
                        <a href="/warningListDetail/<%=data[i].warning_code%>">
                        <%= data[i].warningedID %>
                        </a>
                    </td>
                    <td>
                        <a href="/warningListDetail/<%=data[i].warning_code%>">
                        <%= data[i].warningID %>
                        </a>
                    </td>
                    <td>
                        <a href="/warningListDetail/<%=data[i].warning_code%>">
                        <%= data[i].warning_date %>
                        </a>
                    </td>
            </tr>
        <%}%>
    <%}%>
</tbody>
</table>